<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleHead">
        <attr name="circleBorderHeadWidth" format="dimension" />
        <attr name="ringHeadColor" format="color" />
        <attr name="backgroundHeadColor" format="color" />
    </declare-styleable>

    <declare-styleable name="CutTextView">
        <attr name="start" format="integer" />
        <attr name="end" format="integer" />
        <attr name="marginStart" format="dimension" />
        <attr name="marginEnd" format="dimension" />
        <attr name="normalColor" format="color" />
        <attr name="clickColor" format="color" />
        <attr name="clickIndex" format="integer" />
        <attr name="cutText" format="string" />
    </declare-styleable>
</resources>
